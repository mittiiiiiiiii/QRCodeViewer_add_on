{
    "name": "QRCodeViewer",
    "version": "0.1.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "qr_viewer.html",
        "default_icon": "qrコード.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            }
        }
    },
    "permissions": ["scripting", "tabs"],
    "host_permissions": ["http://*/*", "https://*/"]
}